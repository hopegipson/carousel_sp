 hidden;\n  pointer-events: none;\n  transform-origin: center;\n}\n.menu--item .btn {\n  z-index: 2;\n  transform: translateX(-50%);\n  line-height: 1.5vh;\n  position: absolute;\n  top: 90%;\n  padding: 0.6vh;\n  display: inline-block;\n  text-align: center;\n  text-decoration: none;\n  vertical-align: middle;\n  font-size: calc(9px + 0.2vw);\n  transition: color 0.15s ease-in-out, background-color 0.15s ease-in-out,\n    border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;\n}\n.menu--item figure img {\n  position: absolute;\n  z-index: 1;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  vertical-align: middle;\n  transform-origin: center;\n}\n.album-rotator {\n  width: 100%;\n  float: left;\n  overflow: hidden;\n  position: relative;\n  padding: 2rem 0;\n}\n.album-rotator-holder {\n  position: relative;\n  user-select: none;\n  will-change: transform;\n  height: 50vh;\n}\n\n.navigation_container {\n  width: 40%;\n  margin: auto;\n}\n\n.quote-rotator {\n  width: 100%;\n  float: left;\n  overflow: hidden;\n  position: relative;\n  padding: 2rem 0;\n}\n.quote-rotator-holder {\n  height: 420px;\n  position: relative;\n  user-select: none;\n  will-change: transform;\n  min-height: 400px;\n}\n\n.quotemenu {\n  overflow: hidden;\n  cursor: grab;\n  position: relative;\n  z-index: 1;\n  height: 100%;\n}\n.quotemenu.is-dragging {\n  cursor: grabbing;\n}\n.quotemenu--wrapper {\n  display: flex;\n  position: absolute;\n  z-index: 1;\n  height: 100%;\n  top: 0;\n  left: 0;\n}\n.quotemenu--item {\n  position: absolute;\n  z-index: 1;\n  top: 0;\n  left: 0;\n  width: 28vh;\n  height: 97%;\n  min-width: 240px;\n}\n.quotemenu--item figure {\n  border-radius: 3px;\n  position: absolute;\n  z-index: 1;\n  border: none;\n  outline: none;\n  box-shadow: none;\n  cursor: pointer;\n  overflow: hidden;\n  pointer-events: none;\n  transform-origin: center;\n  width: 390px;\n}\n.quotemenu--item .quotephoto {\n  object-fit: scale-down;\n  height: 20vh;\n  width: 400px;\n}\n.quotemenu--item figure img {\n  position: absolute;\n  z-index: 1;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  vertical-align: middle;\n  transform-origin: center;\n}\n\n@media screen and (max-width: 2000px) {\n  .menu--item {\n    width: 400px;\n  }\n  .quotemenu--item {\n    width: 400px;\n  }\n  .quotemenu--item figure {\n    width: 390px;\n  }\n  .quote-band-name {\n    position: absolute;\n    right: 0px;\n    bottom: 20px;\n    font-size: 11px;\n  }\n  .quote-text {\n    font-size: 14px;\n  }\n}\n\n/* ipad */\n@media screen and (max-width: 769px) {\n  .menu--item {\n    width: 350px;\n  }\n}\n/* phone laying down maybe also 567 */\n@media screen and (max-width: 479px) {\n  .menu--item {\n    width: 250px;\n  }\n\n  .quotemenu--item {\n    width: 350px;\n  }\n  .quotemenu--item figure {\n    width: 340px;\n  }\n  .quote---text-wrapper {\n    height: 120px;\n  }\n\n  .quote-band-name {\n    font-size: 8px;\n  }\n  .quote-text {\n    font-size: 10px;\n  }\n}\n\n/* phone vertical  */\n@media screen and (max-width: 319px) {\n  .menu--item {\n    width: 250px;\n  }\n}\n\n.visible_wrapper {\n  transition: opacity 0.5s ease;\n  opacity: 100%;\n}\n"},"lineCount":269}},"error":null,"hash":"d6e76e1e457be43383dafc28df9de480","cacheData":{"env":{}}}